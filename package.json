{
    "name": "quickstart-webpack",
    "version": "1.0.0",
    "description": "A scaffold project for a Webpack based webapplication, without the overhead of specific libraries.",
    "main": "index.js",
    "scripts": {
        "build": "npm run clean && npm run lint && npm run dist",
        "dist": "webpack --config webpack.prod.js",
        "start": "webpack-dev-server --config webpack.dev.js",
        "clean": "del-cli dist/bundle.js dist/bundle.js.map dist/*.bmp dist/*.png dist/*.svg dist/*.eot dist/*.ttf dist/*.woff dist/*.woff2 *.tgz",
        "lint": "eslint ./src --ext .js,.jsx",
        "lint:fix": "eslint ./src --ext .js,.jsx --fix",
        "prepack": "npm run clean && npm run dist",
        "watch": "cross-env NODE_ENV=development webpack --config webpack.dev.js --mode development --watch --progress "
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@popperjs/core": "^2.9.2",
        "bootstrap": "^5.0.2",
        "cross-env": "^7.0.3",
        "imports-loader": "^3.0.0",
        "jquery": "^3.6.0",
        "qs": "^6.5.2",
        "querystring-es3": "^0.2.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-wordcloud": "^1.2.7",
        "suggestor1": "file:../wordcloud/client"
    },
    "devDependencies": {
        "@babel/core": "^7.8.0",
        "@babel/plugin-transform-runtime": "^7.10.3",
        "@babel/preset-env": "^7.8.0",
        "@babel/preset-react": "^7.14.5",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "autoprefixer": "^10.2.6",
        "babel-loader": "^8.0.0",
        "css-loader": "^4.3.0",
        "del-cli": "^3.0.0",
        "eslint": "^7.9.0",
        "eslint-plugin-react": "^7.24.0",
        "file-loader": "^6.1.0",
        "mini-css-extract-plugin": "^1.6.0",
        "node-sass": "^6.0.0",
        "postcss-loader": "^6.1.0",
        "sass-loader": "^10.2.0",
        "style-loader": "^1.3.0",
        "url-loader": "^4.1.0",
        "webpack": "^5.40.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^4.0.0",
        "webpack-merge": "^5.8.0"
    }
}
